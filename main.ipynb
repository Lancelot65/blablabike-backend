   "source": [
    "from utils import ParquetLoader, GPSSimplifier, GPSConverter\n",
    "\n",
    "GPSConverter(\"b.gpx\")\n",
    "ParquetLoader().save(GPSSimplifier(30).simplify(ParquetLoader().load(\"b.parquet\")), \"b_simplified.parquet\")"
   ]
